  "description": "AxonStreamAI Backend - NestJS with Fastify, PostgreSQL, Redis",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
